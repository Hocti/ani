{
	"name": "ani-lib",
	"version": "1.0.0",
	"description": "Tiny animation library like jquery animate function chain, with more features",
	"module": "./dist/ani.esm.js",
	"bundle": "./dist/ani.js",
	"types": "./dist/ani.d.ts",
	"namespace": "ani",
	"exports": {
		".": {
			"import": {
				"types": "./dist/ani.d.ts",
				"default": "./dist/ani.esm.js"
			},
			"require": {
				"types": "./dist/ani.d.ts",
				"default": "./dist/ani.cjs.js"
			}
		}
	},
	"author": "hocti",
	"license": "MIT",
	"keywords": ["animation", "animate", "chain"],
	"readme": "README.md",
	"bugs": "https://github.com/Hocti/ani/issues",
	"scripts": {
		"watch": "cross-env NODE_ENV=DEV rollup -c --watch",
		"build": "cross-env NODE_ENV=production rollup -c"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/Hocti/ani/"
	},
	"files": [
		"./dist/",
		"LICENSE",
		"package.json",
		"README.md"
	],
	"devDependencies": {
		"@rollup/plugin-commonjs": "^25.0.7",
		"@rollup/plugin-node-resolve": "^15.2.3",
		"@types/jest": "^29.5.11",
		"cross-env": "^7.0.3",
		"esbuild": "^0.19.9",
		"jest": "^29.7.0",
		"rollup": "^4.9.0",
		"rollup-plugin-dts": "^6.1.0",
		"rollup-plugin-esbuild": "^6.1.0",
		"ts-jest": "^29.1.1",
		"typescript": "^5.3.3"
	}
}
